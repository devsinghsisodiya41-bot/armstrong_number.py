#include <stdio.h>

int main() {

    // For loop
    for (int i = 1; i <= 5; i++) {
        printf("%d ", i);
    }
    printf("\n");

    // While loop
    int count = 1;
    while (count <= 5) {
        printf("%d ", count);
        count++;
    }
    printf("\n");

    // Do-while loop
    int num = 1;
    do {
        printf("%d ", num);
        num++;
    } while (num <= 5);
    printf("\n");

    return 0;
}